{
    "name": "redaxo/redaxo",
    "description": "REDAXO, a PHP-based CMS since 2004. Both simple and flexible.",
    
    "require": {
        "erusev/parsedown": "^1.6",
        "erusev/parsedown-extra": "^0.7",
        "filp/whoops": "^2.2.1",
        "psr/log": "^1.0",
        "ramsey/http-range": "dev-legacy",
        "roave/security-advisories": "dev-master",
        "symfony/console": "^4.2",
        "symfony/polyfill-mbstring": "^1.9",
        "symfony/var-dumper": "^4.2",
        "symfony/yaml": "^4.2"
    },

    "config": {
        "vendor-dir": "redaxo/src/core/vendor",
        "autoloader-suffix": "RedaxoCore",
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.1.3"
        }
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/redaxo/http-range"
        }
    ]
}
